# Callway local development environment variables (sample)

# Express server
PORT=3001
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Public URL of this server (used for webhooks / callbacks)
PUBLIC_APP_URL=http://localhost:3001

# OpenAI Realtime
OPENAI_API_KEY=sk-...
OPENAI_REALTIME_MODEL=gpt-realtime
REALTIME_MIN_COMMIT_MS=120
REALTIME_MAX_COMMIT_MS=800
REALTIME_USE_SERVER_VAD=0

# Supabase (server-side only)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Telnyx Voice API
TELNYX_API_KEY=KEYxxxxxxxxxxxxxxxx
TELNYX_CONNECTION_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
TELNYX_OUTBOUND_CALLER_ID=+33123456789
TELNYX_PUBLIC_KEY=base64publickeyhere
# Optional: override the stream WebSocket endpoint used by Telnyx (defaults to PUBLIC_APP_URL + /api/telnyx/stream)
TELNYX_STREAM_URL=

# Phone parsing
DEFAULT_PHONE_REGION=FR
PHONE_FALLBACK_REGIONS=FR,BE,CH,DE,AT,LU,NL,IE,GB,ES,PT,IT,DK,SE,NO,FI,US,CA
